{
    "name": "FyBar",
    "version": "1.0",
    "manifest_version": 2,
    "icons": {"128": "icon_128.png"},
    "description": "Spotify Web Player chrome bar for easy controls.",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "fybar.html"
    },
    "permissions": ["activeTab"]
}